{
  "name": "serverless-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env IS_OFFLINE=1 concurrently --kill-others \"yarn start:devserver\" \"yarn start:offline\"",
    "start:devserver": "webpack serve --config webpack.browser.config.js --mode development",
    "start:offline": "sls offline start",
    "build:serverless": "sls package",
    "build:browser": "webpack --config webpack.browser.config.js --mode production",
    "deploy": "sls deploy --aws-profile learn",
    "test": "jest",
    "lint": "yarn check-types eslint",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "core-js": "^3.19.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "source-map-support": "^0.5.20"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/preset-typescript": "^7.15.0",
    "@types/aws-lambda": "^8.10.84",
    "@types/babel-core": "^6.25.7",
    "@types/babel__core": "^7.1.16",
    "@types/babel__preset-env": "^7.9.2",
    "@types/concurrently": "^6.3.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/core-js": "^2.5.5",
    "@types/file-loader": "^5.0.1",
    "@types/jest": "^27.0.2",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/serverless": "^1.78.38",
    "@types/source-map-support": "^0.5.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.3.1",
    "@typescript-eslint/parser": "^5.2.0",
    "aws-sdk": "^2.1015.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-module-name-mapper": "^0.1.5",
    "mini-css-extract-plugin": "^2.4.3",
    "null-loader": "^4.0.1",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "serverless": "^2.64.1",
    "serverless-offline": "^8.2.0",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-s3-deploy": "^0.10.1",
    "serverless-webpack": "^5.5.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-stats-plugin": "^1.0.3"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always"
  }
}
